{
  "extends": "tslint:latest",
  "rules": {
    "no-bitwise": false,
    "quotemark": [true, "single", "avoid-escape"],
    "variable-name": [
      true, "ban-keywords", "check-format", "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ]
  }
}